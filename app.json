{
  "expo": {
    "name": "Iconik Pro Gym",
    "slug": "iconik-pro-gym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E31C1F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iconikprogym.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tuempresa.iconikprogym",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-sqlite",
      "@react-native-firebase/app",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci칩n necesita acceso a tus fotos para seleccionar im치genes de ejercicios.",
          "cameraPermission": "La aplicaci칩n necesita acceso a la c치mara para tomar fotos de ejercicios."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.tuempresa.iconikprogym"
        }
      ]
    ],
    "jsEngine": "jsc"
  }
}
